    "entrypoints": {
        "main": [
            "li.cil.scannable.common.fabric.CommonSetupFabric"
        ],
        "client": [
            "li.cil.scannable.client.fabric.ClientSetupFabric"
        ]
    },
    "depends": {
        "fabric-api": ">=${fabricApiVersion}",
        "forgeconfigapiport": ">=${forgeConfigPortVersion}",
        "architectury": ">=${architecturyVersion}",
