{
    "schemaVersion": 1,
    "id": "scannable",
    "version": "${version}",
    "name": "Scannable",
    "description": "Scan the area for points of interest.",
    "authors": [
        "Sangar",
        "embeddedt"
    ],
    "contact": {
        "sources": "https://github.com/MightyPirates/Scannable"
    },
    "mixins": [
        "scannable-common.mixins.json",
        "scannable.mixins.json"
    ],
    "license": "MIT",
    "icon": "logo.png",
